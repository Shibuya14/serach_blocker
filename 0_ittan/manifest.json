{
    "manifest_version": 3,
    "name": "Search Blocker",
    "version": "1.0",
    "description": "特定のキーワードを含む検索を防ぐ",
    "permissions": ["storage", "scripting"],
    "host_permissions": ["*://www.google.com/*"],
    "options_page": "options.html",
    "content_scripts": [
    {
        "matches": ["*://www.google.com/search*"],
        "js": ["content.js"]
    }
    ]
}
